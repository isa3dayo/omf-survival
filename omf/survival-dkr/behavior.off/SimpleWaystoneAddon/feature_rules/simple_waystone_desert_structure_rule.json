{
  "format_version": "1.13.0",
  "minecraft:feature_rules": {
    "description": {
      "identifier": "simple_waystone:simple_waystone_desert_structure_rule",
      "places_feature": "simple_waystone:simple_waystone_desert_structure_feature"
    },
    "conditions": {
      "placement_pass": "surface_pass",
      "minecraft:biome_filter": [
        {
          "any_of": [
            { "test": "has_biome_tag", "operator": "==", "value": "desert" },
            { "test": "has_biome_tag", "operator": "==", "value": "desert_hills" },
            { "test": "has_biome_tag", "operator": "==", "value": "desert_mutated" }
          ]
        },
        {
          "all_of": [
            { "test": "has_biome_tag", "operator": "!=", "value": "ocean" },
            { "test": "has_biome_tag", "operator": "!=", "value": "deep" },
            { "test": "has_biome_tag", "operator": "!=", "value": "caves" },
            { "test": "has_biome_tag", "operator": "!=", "value": "deep_dark" }
          ]
        }
      ]
    },
    "distribution": {
      "iterations": 0.5,
      "scatter_chance": 0.5,
      "x": { "distribution": "uniform", "extent": [ 0, 15 ] },
      "y": "query.heightmap(variable.worldx, variable.worldz)",
      "z": { "distribution": "uniform", "extent": [ 0, 15 ] }
    }
  }
}